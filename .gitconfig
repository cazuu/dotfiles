[user]
	email = takahashi21@users.noreply.github.com
	name = takahashi21
[alias]
	tree = log --graph --all --format=\"%x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(magenta reverse)%d%Creset %s\"
        st = status
        ci = commit
        dc = diff --cached
[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
