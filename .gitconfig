[user]
	email  = takahashi21@users.noreply.github.com
	name = takahashi21
[alias]
	tree = log --graph --all --format=\"%x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(magenta reverse)%d%Creset %s\"
        st = status --short --branch
        ci = commit
        dc = diff --cached --color-words
        df = diff --color-words
        br = branch
        ck = checkout
        fp = fetch -p
[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
[core]
	editor = vim -C \"set fenc=utf-8\"
	attributesfile = /Users/takahashi/.gitattributes_global
[commit]
	template = /Users/takahashi/dotfiles/.gitmessage.txt
[color]
	ui = auto
